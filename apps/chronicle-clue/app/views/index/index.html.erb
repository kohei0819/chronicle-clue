<div class="row">
  <div class="col s12">
    <ul>
      <% (1..10).each do |n| %>
        <li>
          <div class="card horizontal">
            <div class="card-image">
              <%= image_tag 'cinema/1.jpg', :size => '130' %>
            </div>
            <div class="card-stacked">
              <div class="card-content">
                <p>No.<%= n %> I am a very simple card. I am good at containing small bits of information.</p>
              </div>
              <div class="card-action">
                <a href="#">This is a link</a>
              </div>
            </div>
          </div>
        </li>
      <% end %>
    </ul>
  </div>
</div>
